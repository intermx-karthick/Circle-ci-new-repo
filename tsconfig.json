{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "downlevelIteration": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2019",
      "dom"
    ],
    "baseUrl": "src",
    "paths": {
      "@shared/*": [
        "app/shared/*"
      ],
      "@d3/*": [
        "app/d3/*"
      ],
      "@interTypes/*": [
        "app/Interfaces/*"
      ],
      "@constants/*": [
        "app/constants/*"
      ]
    },
    "module": "es2020"
  }
}
